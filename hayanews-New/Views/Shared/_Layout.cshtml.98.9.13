@using System.Web.Mvc.Html
@using Comman.Helpers
@using Models
@using Resources
@using Resources.App_GlobalResources
@{
    var keys = new List<string>() { "webmaster", "googleanaletics" };
    string s = Html.Action("GetContentsValueByKeys", "ContentLocalized", new { keys = keys, ishtml = true }).ToString();
    var result = new System.Web.Script.Serialization.JavaScriptSerializer()
                                        .Deserialize<List<ContentItems>>(s);
}
<!DOCTYPE html>

<html dir="@ResourceLocalization.GetResource(()=>Resource.dir)">
<head>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    @{
        var key = "webmaster";
        {
            var firstOrDefault = result.FirstOrDefault(x => x.Key == key);
            if (firstOrDefault != null)
            {
                WriteLiteral(firstOrDefault.Value);
            }
        }
    }
    @if (IsSectionDefined("robots"))
    {
        @RenderSection("robots", false)
    }
    else
    {
        <meta name="robots" content="index, follow" />
    }
    @if (IsSectionDefined("Paging"))
    {
        @RenderSection("Paging", false)
    }
    @if (IsSectionDefined("Title"))
    {
        @RenderSection("Title", false)
    }
    else
    {
        @Html.Action("GetDefaultPagesTitle", "Home")
    }
    @if (IsSectionDefined("head"))
    {
        @RenderSection("head", false)
    }
    else
    {
        @Html.Action("GetDefaultMeta", "Home")
    }
    @*@{ Html.RenderPartial("_FavIconPartial");}*@
    @*<link href="/GlobalItems/css/custom-shared.css" rel=stylesheet />*@
    <link href="/GlobalItems/fonts/materialFontIcon/css/materialdesignicons.min.css" rel="stylesheet" media="all" />

    @if (IsSectionDefined("cssTop"))
    {
        @RenderSection("cssTop", false)
    }
    else
    {
        <!-- materialdesignicons -->
        <link href="/GlobalItems/fonts/materialFontIcon/css/materialdesignicons.min.css" rel="stylesheet">
        <!-- Bootstrap -->
        <link href="/themes/hayanews/static/css/bootstrap.min.css" rel="stylesheet">
        <!-- base -->
        <link href="/themes/hayanews/static/css/style.css" rel="stylesheet">
        <!-- menu-resize -->
        <link rel="stylesheet" type="text/css" href="/themes/hayanews/static/css/menuResize/menuResize.css" media="all" />
        <link rel="stylesheet" type="text/css" href="/themes/hayanews/static/css/menu/menu.css" media="all" />
        <!--carousel-->
        <link rel="stylesheet" type="text/css" href="/themes/hayanews/static/css/carousel/owl.carousel.css" media="all" />
        <link rel="stylesheet" type="text/css" href="/themes/hayanews/static/css/carousel/owl.theme.css" media="all" />
        <!-- form-material-->
        <link type="text/css" rel="stylesheet" href="/GlobalItems/form-material/style/form-material.css" media="all" />
    }
    <link href="/GlobalItems/viget/fancy-in/lightcase.css" rel="stylesheet" />
    <link rel="stylesheet" type="text/css" href="/GlobalItems/viget/viget-rtl.css" media="all" />
</head>
<body>
    @RenderSection("Banner", false)

    @{ Html.RenderPartial("_HeaderPartial");}
    @RenderBody()

    @{ Html.RenderPartial("_FooterPartial");}
    @Html.Action("GetThemesSettingsForShowInput", "ThemesSettings")

    @if (IsSectionDefined("jsBottom"))
    {
        @RenderSection("jsBottom", false)
    }
    else
    {
        <!--================================ js ====================================-->
        <!-- start script -->
        <script type="text/javascript" src="/themes/hayanews/static/js/jquery.min.js"></script>
        <script type="text/javascript" src="/themes/hayanews/static/js/bootstrap.min.js"></script>
        <!-- ripplink -->
        <script src="/themes/hayanews/static/js/ripplink.js"></script>
        <!-- menu -->
        <script src="/themes/hayanews/static/js/menu/bootstrap-dropdownhover.js"></script>
        <!-- custom -->
        <script src="/themes/hayanews/static/js/custom.js"></script>
        <!--carousel-->
        <script src="/themes/hayanews/static/js/carousel/owl.carousel.js"></script>
        <!--backToTop-->
        <script src="/themes/hayanews/static/js/backToTop/backToTop.js"></script>
        <!-- material-->
        <script type="text/javascript" src="/GlobalItems/form-material/style/form-material.js"></script>
        <script src="@Url.Content("~/Areas/GlobalItems/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Areas/GlobalItems/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>


    }


    <script src="@Url.Action("GeneralJs", "Home")"></script>
    <script type="text/javascript" src="@Url.RouteUrl("HomeEnRout", new{Controller = "home",Action = "LiveSearch",liveSearchUrl = Url.Action("ScoredTerms","Search"),targetInput="lSerchTerm",width="auto",dir=ResourceLocalization.GetResource(() => Resource.dir)})"></script>


    @if (Request.IsAuthenticated && (Roles.GetRolesForUser().Contains(Settings.AdminRole) || Roles.GetRolesForUser().Contains(Settings.ProgrammerRole)))

    {
        Html.RenderPartial(Url.Content("/Themes/Views/SharedItems/_EditableConfigs.cshtml"));
        @Html.Action("ThemeSettingsChange", "ThemesSettings", new { area = "Admin" })
    }
    @if (IsSectionDefined("onLoad"))
    {
        @RenderSection("onLoad", false)
    }
    else
    {
    }
    @if (Request.IsAuthenticated && (Roles.GetRolesForUser().Contains(Settings.AdminRole) || Roles.GetRolesForUser().Contains(Settings.ProgrammerRole)))
    {
        Html.RenderPartial(Url.Content("/Themes/Views/SharedItems/_ManagmentHeader.cshtml"));
    }
    @{
        key = "googleanaletics";
        {
            var firstOrDefault = result.FirstOrDefault(x => x.Key == key);
            if (firstOrDefault != null)
            {
                WriteLiteral(firstOrDefault.Value);
            }
        }
    }
    <script>
        loadCaptcha();
        loadCaptcha('m_imgCaptchaFooter');


    </script>

</body>
</html>