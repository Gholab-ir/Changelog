.show-filter {
    position: relative;
    float: left;
    padding-left: 20px ;
    padding-right: 20px;
}
.show-filter span {
    font-size: 9pt;
}
.show-filter .select-num {
    position: relative;
    display: inline-block;
    line-height: 23px;
    box-shadow: 0px 0px 2px rgba(17, 17, 17, 0.2);
    padding: 0px 8px;
    margin-right: 5px;
    width: 50px;
    cursor: pointer;
    transition: all 0.3s;
}
.show-filter .select-num.active {
    background-color: #fff;
}
.show-filter .select-num i {
    position: absolute;
    font-size: 11pt;
    top: 0;
    left: 2px;
}
.show-filter > ul {
    position: absolute;
    left: 20px;
    width: 50px;
    background-color: #fff;
    z-index: 20;
    top: 23px;
    display: none;
    box-shadow: 0px 2px 2px 0px rgba(0, 0, 0, 0.2); 
}
.show-filter > ul > li {
    cursor: pointer;
    padding-right: 8px;
    transition: all 0.5s;
    background-color: #fff;
}
.show-filter > ul > li:hover,
.show-filter > ul > li.selected {
    background-color: #fa4949;
    color: #fff;
}







