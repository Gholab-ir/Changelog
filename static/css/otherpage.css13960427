/*news list2*/
.paging-model2 .paging{
    border:0!important;
    padding:0!important
}
.paging-model2 .paging>ul{
    float:left;
    margin:auto;
}
.paging-model2 .paging>ul>li{
    position:relative;
    display:inline-block;
    vertical-align:middle;
    width:24px;
    height:24px;
    margin:0 1px;
    -webkit-transition:all .4s ease-in-out;
    -moz-transition:all .4s ease-in-out;
    -ms-transition:all .4s ease-in-out;
    -o-transition:all .4s ease-in-out;
    transition:all .4s ease-in-out;
    -webkit-box-shadow:0 0 3px 0 rgba(0,0,0,.2);
    -moz-box-shadow:0 0 3px 0 rgba(0,0,0,.2);
    box-shadow:0 0 3px 0 rgba(0,0,0,.2);
    -moz-border-radius:50%;
    -webkit-border-radius:50%;
    -khtml-border-radius:50%;
    border-radius:50%;
    text-align:-moz-center;
    text-align:-webkit-center;
    text-align:center
}
.paging-model2 .paging>ul>li:hover{
    background:#fa4949
}
.paging-model2 .paging>ul>li:hover a{
    color:#fff
}
.paging-model2 .paging>ul>li a{
    color:#515351;
    font-size:8.5pt;
    line-height:2.3;
    display:block
}
.paging-model2 .paging>ul>li:first-child a{
    font-family:"Material Design Icons";
    font-size:11pt
}
.paging-model2 .paging>ul>li:first-child a:before{
    content:"\F142";
    position:absolute;
    left:0;right:-1px;
    margin:auto;top:-5px
}
.paging-model2 .paging>ul>li:last-child a{
    font-family:"Material Design Icons";
    font-size:11pt
}
.paging-model2 .paging>ul>li:last-child a:before{
    content:"\F141";
    position:absolute;
    left:0;right:1px;
    margin:auto;
    top:-5px
}
.paging-model2 .paging>ul>li.active{
    background:#fa4949;
    margin-top:-3px
}
.paging-model2 .paging>ul>li.active a{
    color:#fff
}


