.sort-list {
    float: left;
    width: 173px;
}
.sort-list span {
    position: relative;
    display: inline-block;
    line-height: 25px;
    margin-right: 3px;
    cursor: pointer;
    font-size: 9pt;
}
.sort-list span::after {
    content: "";
    position: relative;
    top: 5px;
    display: inline-block;
    height: 14px;
    width: 14px;
    border-radius: 50%;
    margin-right: 5px;
    border: 1px solid #c2c7cc;
    transition: all 0.3s;
}
.sort-list span.selected:after {
    border-color: #fa4949;
}
.sort-list span:before {
    content: "";
    position: absolute;
    top: 9px;
    left: 3px;
    display: block;
    height: 8px;
    width: 8px;
    border-radius: 50%;
    cursor: pointer;
    background-color: #fa4949;
    opacity: 0;
    transition: all 0.3s;
}
.sort-list span.selected:before  {
    opacity: 1;
}
